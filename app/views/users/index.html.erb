<main>
    <div> 
    <%= render 'user', book: @book, user: @user %>
    <table class="table table-striped">
        <h1>Users</h1>
        <% @users.each do |user| %>
            <p>image</p><br><%= image_tag user.get_profile_image(100,100) %>
            <p>name <br><%= user.name %></p><br><%= link_to "Show", user_path(user.id) %>
        <% end %>
    </table>
    </div>
</main>